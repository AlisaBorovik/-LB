#include <iostream>
using namespace std;

int main() {
    const int m = 5;
    const int n = 7;
    int a[m][n];

    cout << "Enter elements of 5x7 array:" << endl;
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            cin >> a[i][j];
        }
    }

    for (int i = 0; i < m; i++) {
        int sum = 0;
        for (int j = 0; j < n; j++) {
            sum += a[i][j];
        }
        cout << "Sum of row " << i << " is " << sum << endl;
    }

    return 0;
}
