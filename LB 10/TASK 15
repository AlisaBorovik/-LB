#include <iostream>
using namespace std;

int main() {
    const int m = 3;
    const int n = 3;
    int a[m][n];

    cout << "Enter elements of 3x3 array:" << endl;
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            cin >> a[i][j];
        }
    }

    int sumDiag = 0;

    for (int i = 0; i < m; i++) {
        sumDiag += a[i][i];
    }

    cout << "Sum of main diagonal: " << sumDiag << endl;

    return 0;
}

