#include <iostream>
using namespace std;

int main() {
    const int m = 4;
    const int n = 6;
    int a[m][n];

    cout << "Enter elements of 4x6 array:" << endl;
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            cin >> a[i][j];
        }
    }

    for (int j = 0; j < n; j++) {
        int sum = 0;
        for (int i = 0; i < m; i++) {
            sum += a[i][j];
        }
        cout << "Sum of column " << j << " is " << sum << endl;
    }

    return 0;
}

