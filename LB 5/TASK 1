#include <iostream>
#include <cmath>
using namespace std;

int main() {
    cout << "y = sin^5(x) + |5x - 1.5|" << endl;
    double x;

    for (int i = 0; i < 5; i++) {
        cout << "Enter x" << ": ";
        cin >> x;

        double y = pow(sin(x), 5) + fabs(5 * x - 1.5);
        cout << "y" << " = " << y << endl;
    }
    
}
