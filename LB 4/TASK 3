#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

int main() {
    int x, y;
    cout << "Enter two integers: ";
    cin >> x >> y;

    float a = 1.0 / x;
    float b = 1.0 / y;

    float epsilon = 0.000001;

    if (fabs(a - b) < epsilon)
        cout << "Results are equal" << endl;
    else
        cout << "Results are not equal " << endl;

    return 0;
}
