#include <iostream>
using namespace std;

int main() {
   double x, y;
    cout << "Enter X: ";
    cin >> x;
    cout << "Enter Y: ";
    cin >> y;
    bool SqUp = (x>=-1 && x<=1 && y<=1 && y>=0);
    bool SqDown = (x>=0 && x<=1 && y<=0 && y>=-1);

    if (SqUp||SqDown)
        cout << " The point belongs to the given coordinates";
        else
            cout << " The point not belongs to the given coordinates";

    return 0;
}
