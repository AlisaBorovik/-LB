#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main() {
    const int m = 12;
    int a[m];

    srand(time(0));

    for (int i = 0; i < m; i++) {
        a[i] = rand() % 10 + 1;
    }

    cout << "Generated array: ";
    for (int i = 0; i < m; i++) {
        cout << a[i] << " ";
    }
    cout << endl;

    int max1 = 0;
    int max2 = 0;

    for (int i = 0; i < m; i++) {
        if (a[i] > max1) {
            max2 = max1;
            max1 = a[i];
        } else if (a[i] > max2 && a[i] < max1) {
            max2 = a[i];
        }
    }

    cout << "Second largest element is: " << max2 << endl;

    return 0;
}
